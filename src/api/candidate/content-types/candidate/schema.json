{
  "kind": "collectionType",
  "collectionName": "candidates",
  "info": {
    "singularName": "candidate",
    "pluralName": "candidates",
    "displayName": "Candidate"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "votes": {
      "type": "biginteger",
      "required": true,
      "default": "0"
    },
    "avatar": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "campus": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::campus.campus",
      "inversedBy": "candidates"
    },
    "student": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::student.student"
    }
  }
}
