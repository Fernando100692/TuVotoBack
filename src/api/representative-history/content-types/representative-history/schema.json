  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "grade": {
      "type": "string"
    },
    "group": {
      "type": "string"
    },
    "campus": {
      "type": "string"
    },
    "votes": {
      "type": "integer"
    },
    "session": {
      "type": "string"
    },
    "isActive": {
      "type": "boolean",
      "default": true
